{
  "name": "the-drunken-koth",
  "description": "A application to manage and record king of the hill mtg tournaments in local stores.",
  "version": "1.0.0",
  "private": true,
  "author": "Gustavo da Silva Barcaro <GustavoBarcaro/gubarcaro@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "1.7.2",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "axios": "0.24.0",
    "bcryptjs": "2.4.3",
    "faunadb": "4.4.1",
    "framer-motion": "4",
    "next": "12.0.3",
    "next-auth": "3.29.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "react-ink": "6.5.1",
    "react-toastify": "8.1.0",
    "sass": "1.43.4",
    "swr": "1.0.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@types/uuid": "8.3.3",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.3",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1",
    "typescript": "4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
